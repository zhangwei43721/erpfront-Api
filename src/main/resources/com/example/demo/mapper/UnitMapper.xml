<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.UnitMapper">

    <resultMap id="BaseResultMap" type="com.example.demo.pojo.Unit">
            <id property="unitId" column="unit_id" />
            <result property="unitName" column="unit_name" />
            <result property="unitDesc" column="unit_desc" />
    </resultMap>

    <sql id="Base_Column_List">
        unit_id,unit_name,unit_desc
    </sql>
</mapper>
